{-# STDLIB_VERSION 3 #-}
{-# CONTENT_TYPE EXPRESSION #-}
{-# SCRIPT_TYPE ASSET #-}

let val = this.issuer.getBinaryValue("1")
let source = val # + val

let proof = base64''
let pubKey = base64''

rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
&& rsaVerify(SHA3512, source, proof, pubKey)
